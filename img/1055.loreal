<script>
    var initPri;
    $(function () {
        initPri = {
            timeTemp: '',
            agreePri: false,
            site: {
                url: window.location.href.split('#')[0]
            },
            hascookiePrivacy: function () {
                //cookiePrivacy.deleteCookie('cookieForPrivacy2018')
                var _cookiePrivacy = cookiePrivacy.getCookie('cookieForPrivacy2018')
                if (_cookiePrivacy) {
                    return true;
                } else {
                    return false;
                }
            },
            _getPriPcy: function () {
                $.ajax({
                    url: "/OapApi/GetCustomerPrivacyPolicy.loreal",
                    type: "post",
                    dataType: "json",
                    cache: false,
                    data: {
                    },
                    success: function (msg) {
                        if (msg.success == true) {
                            if (!msg.customerPrivacyPolicy) {
                                $('.LogAndReg_Box').fadeIn();
                            } else {
                                initPri.agreePri = true;
                                cookiePrivacy.SetCookie('cookieForPrivacy2018', '1')
                            }
                        }
                    }, error: function () {
                        return "";
                    }
                })
            },
            _savePriPcy: function () {
                $.ajax({
                    url: "/OapApi/SaveCustomerPrivacyPolicy.loreal",
                    type: "post",
                    dataType: "json",
                    cache: false,
                    data: {
                        url: initPri.site.url
                    },
                    success: function (msg) {
                        $('.LogAndReg .LogAndReg_Box').fadeOut();
                        initPri.agreePri = true;
                        cookiePrivacy.SetCookie('cookieForPrivacy2018', '1')
                    }, error: function () {
                        return "";
                    }
                })
            }

        }

        $('.LogAndReg .LogAndReg_Box_close,.LogAndReg_Boxagree_agree').on('click', function () {
            if (initPri.timeTemp == null) {
                initPri.timeTemp = new Date().getTime();
            } else {
                var t2 = new Date().getTime();
                if (t2 - initPri.timeTemp < 5000) {
                    initPri.timeTemp = t2; return;
                } else { initPri.timeTemp = t2; }
            }
            initPri._savePriPcy();
        })
        if (!initPri.hascookiePrivacy()) {
            initPri._getPriPcy()
        }
        $('.LogAndReg .toPrivacy').on('click', function () {
            window.open('http://policy.lorealchina.com/privacypolicy')
        })
    })
</script>


